{
	"manifest_version": 3,
	"name": "Table Scraper",
	"description": "Downloads tables",
	"version": "1.0",
	"permissions": ["activeTab", "scripting", "downloads"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": "img/table.png",
		"default_title": "Download Tables"
	},
	"icons": {
		"128": "img/table.png"
	}
  }